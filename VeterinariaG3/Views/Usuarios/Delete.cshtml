@model VeterinariaG3.ViewModels.UsuarioEditarViewModel

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Usuario</h1>

<h3>Are you sure you want to delete this usuario?</h3>
<div>
    <dl class="row">
        <dt class="col-sm-2">Id</dt>
        <dd class="col-sm-10">@Model.Id</dd>
        <dt class="col-sm-2">Email</dt>
        <dd class="col-sm-10">@Model.Email</dd>
        <dt class="col-sm-2">Nombre</dt>
        <dd class="col-sm-10">@Model.Nombre</dd>
        <dt class="col-sm-2">Primer Apellido</dt>
        <dd class="col-sm-10">@Model.PrimerApellido</dd>
        <dt class="col-sm-2">Segundo Apellido</dt>
        <dd class="col-sm-10">@Model.SegundoApellido</dd>
        <dt class="col-sm-2">Estado</dt>
        <dd class="col-sm-10">@Model.Estado</dd>
        <dt class="col-sm-2">Última Conexión</dt>
        <dd class="col-sm-10">@Model.UltimaConexion</dd>
        <dt class="col-sm-2">Imagen</dt>
        <dd class="col-sm-10">@Model.Imagen</dd>
    </dl>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
