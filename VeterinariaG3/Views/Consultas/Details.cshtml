@model Veterinaria.DAL.Consulta

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles de la Consulta</h1>

<div>
    <h4>Consulta</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateTimeValue)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateTimeValue)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Motivo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Motivo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Diagnostico)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Diagnostico)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Tratamiento)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Tratamiento)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Medicamentos)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Medicamentos)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdConsulta" class="btn btn-primary">Editar</a> |
    <a asp-action="Index" class="btn btn-secondary">Volver a la Lista</a>
</div>
